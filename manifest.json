{
    "manifest_version": 2,

    "name": "Satisfaction",

    "version": "1.0",

    "background": {
        "page": "test1.html"
    },

    "content_scripts": [
        {
            "matches": ["https://www.amazon.com/*", "http://www.amazon.com/*"],
            "js": ["jquery-1.11.2.js", "jquery-ui.min.js", "contentscript.js"],
            "run_at": "document_end"
        }
    ],

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "permissions": [
        "tabs",
        "http://*/*",
        "activeTab",
        "https://ajax.googleapis.com/"
    ]
}
